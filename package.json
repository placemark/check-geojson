{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "husky install && tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@placemarkio/check-geojson",
  "author": "Tom MacWright",
  "module": "dist/check-geojson.esm.js",
  "size-limit": [
    {
      "path": "dist/check-geojson.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/check-geojson.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "husky": "^7.0.1",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@humanwhocodes/momoa": "^2.0.2",
    "@types/geojson": "^7946.0.7",
    "type-fest": "^1.2.1"
  }
}
